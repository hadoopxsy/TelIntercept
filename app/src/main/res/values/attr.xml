<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingCheckView">
        <attr name="allowDivider" format="boolean"/>
        <attr name="desc" format="string"/>
        <attr name="isOpen"/>
        <attr name="title"/>
    </declare-styleable>

    <declare-styleable name="SettingArrowView">
        <attr name="title"/>
        <attr name="allowDivider"/>
        <attr name="desc"/>
        <attr name="hasArrow" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="slideswitch">
        <attr name="themeColor" format="color" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="TitleDescView">
        <attr name="title"/>
        <attr name="desc"/>
    </declare-styleable>
</resources>